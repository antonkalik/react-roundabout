{
  "name": "react-roundabout",
  "version": "0.0.0",
  "description": "Carousel React component",
  "main": "lib/roundabout.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass src/styles.scss lib/roundabout.min.css --output-style=compressed --source-map=true",
    "eslint": "eslint src/**/*.js",
    "browserify": "browserify -i react -i prop-types src/index.js -o lib/roundabout.js -t [ babelify --presets [ es2015 react ] ]",
    "build": "npm-run-all eslint browserify sass",
    "example:build": "browserify example/index.js -o example/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "example:serve": "http-server example -p 6789 -o",
    "example": "npm-run-all example:build example:serve"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fknussel/react-roundabout.git"
  },
  "keywords": [
    "react-roundabout",
    "react-carousel",
    "react-slider",
    "roundabout",
    "carousel",
    "slider"
  ],
  "author": "Federico Kn√ºssel <fknussel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fknussel/react-roundabout/issues"
  },
  "homepage": "https://github.com/fknussel/react-roundabout#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "react-dom": "^15.5.4"
  },
  "peerDependencies": {
    "prop-types": "^15.5.8",
    "react": "^15.5.4"
  }
}
